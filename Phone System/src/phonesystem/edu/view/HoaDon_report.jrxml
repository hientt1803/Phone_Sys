	<parameter name="MaHoaDon" class="java.lang.String">
		<defaultValueExpression><![CDATA["HD12"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     HoaDon."MaHoaDon" AS HoaDon_MaHoaDon,
     KhachHang."TenKhachHang" AS KhachHang_TenKhachHang,
     NhanVien."TenNhanVien" AS NhanVien_TenNhanVien,
     SanPham."TenSanPham" AS SanPham_TenSanPham,
     KhachHang."SDT" AS KhachHang_SDT,
     HoaDon."NgayTao" AS HoaDon_NgayTao,
     SanPham."DonGia" AS SanPham_DonGia,
     HoaDonChiTiet."SoLuong" AS HoaDonChiTiet_SoLuong,
     HoaDonChiTiet."SoLuong"*SanPham."DonGia" AS ThanhTien,
     HoaDon."TongTien" AS HoaDon_TongTien
FROM
     "dbo"."HoaDon" HoaDon INNER JOIN "dbo"."HoaDonChiTiet" HoaDonChiTiet ON HoaDon."MaHoaDon" = HoaDonChiTiet."MaHoaDon"
     INNER JOIN "dbo"."KhachHang" KhachHang ON HoaDon."MaKhachHang" = KhachHang."MaKhachHang"
     INNER JOIN "dbo"."NhanVien" NhanVien ON HoaDon."MaNhanVien" = NhanVien."MaNhanVien"
     INNER JOIN "dbo"."SanPham" SanPham ON HoaDonChiTiet."MaSanPham" = SanPham."MaSanPham"
WHERE
     HoaDon."MaHoaDon" = $P{MaHoaDon}]]>
	</queryString>
